    "@babel/preset-env": "^7.3.1",
    "@babel/preset-react": "^7.0.0",
    "babel-loader": "^8.0.5",
    "concurrently": "^3.5.1",
    "css-loader": "^3.4.2",
    "html-webpack-plugin": "^3.2.0",
    "prop-types": "^15.7.2",
